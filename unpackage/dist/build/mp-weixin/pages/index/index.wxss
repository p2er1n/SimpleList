.todoList{display:flex;flex-direction:column;align-items:stretch;justify-content:flex-start}.todoItem{display:flex;flex-direction:row;margin-left:20px;margin-bottom:3%;justify-content:space-around;animation-name:slideIn;animation-duration:.7s}.todoItem view{border-radius:30%;width:25px;border:1px solid}.todoItem input{border-bottom:1px solid}@keyframes slideIn{0%{transform:translate(100%)}to{transform:translate(0)}}
